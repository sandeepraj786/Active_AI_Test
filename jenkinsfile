pipeline {
    agent any
    stages {
	    stage('SCM Checkout'){
             steps{    
		    git 'https://github.com/sandeepraj786/Active_AI_Test.git'
		 }
	    }
	   
	   
	    
	    
	    
            stage('Run Ansible playbook') {
               steps {
	               sh ' ansible-playbook /var/lib/jenkins/workspace/Chatbot-Active-Ai-job/ansible/deploy-chatbot-ai.yml'
                     
	             
         }
       }
    }
}
